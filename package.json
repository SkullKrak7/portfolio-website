{
  "name": "portfolio-website",
  "version": "1.0.0",
  "type": "module",
  "description": "Production-ready portfolio website for Sai Karthik Kagolanu",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "audit": "npm audit --audit-level=moderate",
    "size": "size-limit",
    "ci": "npm run type-check && npm run lint && npm run test:coverage && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "size-limit": [
    {
      "path": ".next/static/**/*.js",
      "limit": "200 KB"
    }
  ],
  "keywords": [
    "portfolio",
    "nextjs",
    "ml-engineer",
    "robotics"
  ],
  "author": "Sai Karthik Kagolanu",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.24",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.1",
    "@chromatic-com/storybook": "^5.0.1",
    "@playwright/test": "^1.58.2",
    "@size-limit/preset-app": "^12.0.0",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/nextjs": "^10.2.8",
    "@storybook/nextjs-vite": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "chromatic": "^15.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "^10.2.8",
    "happy-dom": "^20.5.3",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "storybook": "^10.2.8",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
